    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^23.1.0",
    "rimraf": "^2.6.2",
    "testdouble": "^3.6.0"
  },
  "dependencies": {
    "ip": "^1.1.3",
    "ldclient-js": "^2.1.0",
    "lodash": "^4.16.3",
    "react": "^16.4.0",
    "ua-parser-js": "^0.7.10",
    "uuid": "^3.0.1"
